{
  "name": "qa-studio",
  "version": "1.0.0",
  "description": "QA Studio - AI-powered testing workflow automation for QA engineers",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "node --watch server/index.js",
